{
  "name": "fsd-slicer",
  "version": "0.0.1",
  "description": "CLI Feature-Sliced Design structure generator",
  "bin": {
    "fsd-slicer": "dist/slicer.js"
  },
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "tsx src/slicer.ts",
    "prepare": "pnpm run build",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "fsd",
    "feature-sliced",
    "frontend",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Ratch-e/fsd-slicer"
  },
  "author": "Viktor Gunin <guninvk@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "commander": "^14.0.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.3.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.13",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "chalk": "^4.1.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^30.0.4",
    "prettier": "^3.6.2",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
